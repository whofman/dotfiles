{
   "background": {
      "page": "main.html"
   },
   "background_page": "main.html",
   "browser_action": {
      "default_icon": "icons/icon-retina.png",
      "default_popup": "popup.html"
   },
   "commands": {
      "_execute_browser_action": {
         "suggested_key": {
            "default": "Ctrl+Shift+R",
            "linux": "Ctrl+Shift+R",
            "mac": "Alt+Shift+R",
            "windows": "Ctrl+Shift+R"
         }
      }
   },
   "content_scripts": [ {
      "js": [ "content/content.js" ],
      "matches": [ "http://*/*", "https://*/*" ]
   }, {
      "all_frames": true,
      "js": [ "content/page_plugin.js" ],
      "matches": [ "http://*/*", "https://*/*" ],
      "run_at": "document_end"
   }, {
      "js": [ "content/feeder_api.js" ],
      "matches": [ "http://feeder.co/*", "http://*.feeder.co/*", "https://feeder.co/*", "https://*.feeder.co/*" ],
      "run_at": "document_end"
   } ],
   "content_security_policy": "script-src 'self' https://ssl.google-analytics.com https://www.google-analytics.com; object-src 'self'",
   "description": "Get a simple overview of your RSS and Atom feeds in the toolbar",
   "icons": {
      "128": "icons/icon128x128.png",
      "19": "icons/icon-retina.png",
      "48": "icons/icon48x48.png"
   },
   "intents": {
      "http://webintents.org/subscribe": [ {
         "disposition": "window",
         "href": "/options.html?with_subscribe_intent=subscribe",
         "title": "Subscribe to feed using Feeder",
         "type": [ "application/rss+xml", "application/atom+xml" ]
      } ],
      "http://webintents.org/view": [ {
         "disposition": "window",
         "href": "/options.html?with_intent=feed",
         "title": "Subscribe to feed using Feeder",
         "type": [ "application/rss+xml", "application/atom+xml" ]
      } ]
   },
   "key": "MIGfMA0GCSqGSIb3DQEBAQUAA4GNADCBiQKBgQDDSk70DzQ0XRPEINteLpZyjPmFVPgbPePTiy3z2VEWHy8e1hFbOdP32SuiWcshHuZHvCvU3oZdZHJ9h3zKBw100ZzOcIPa8UHxi8klYfwf2QJu+yAYIAm36IlRin8JnwCWIoE7ZcFfLVyXci6LmnIepS69x+asjyPH7zEjmNfcYwIDAQAB",
   "manifest_version": 2,
   "name": "RSS Feed Reader",
   "options_page": "settings_redirect.html",
   "permissions": [ "tabs", "http://*/*", "https://*/*", "chrome://favicon/", "storage", "notifications", "webNavigation", "webRequest", "webRequestBlocking", "unlimitedStorage" ],
   "short_name": "feeder",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "7.7.5",
   "web_accessible_resources": [ "icons/icon48x48.png", "icons/bar-buttons-good_2x.png", "icons/icon-retina.png" ]
}
